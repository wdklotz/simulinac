RBARC=false;
particle, PARTICLE=PROTON;

lqd= 0.4;
lqf=lqd/2.;
ld=0.4;
lcav=0.08;
ldcav=lcav/2.;

dBdz=1.0;
ekin=50.;
etot=(PMASS*1.e+3)+ekin;               //MeV
gamma=etot/(PMASS*1.e+3);
beta=sqrt(1.-1./(gamma*gamma));
kq=299.8*dBdz/(beta*etot);             // m^-2
VALUE, kq, gamma, beta, ekin, etot, dBdz;

QF:   QUADRUPOLE, L=lqf, K1=kq;
QD:   QUADRUPOLE, L=lqd, K1=-kq;
//CAV:  RFCAVITY
//DCAV: DRIFT , L=ldCAV;
DR: DRIFT, L=ld;

TOP: LINE=(QF,DR,DR,QD,DR,DR,QF);

USE, PERIOD=TOP;
SELECT,FLAG=TWISS,COLUMN=NAME,S,BETX,BETY;
TWISS,FILE=twiss.out;
PLOT,RANGE=#S/#E,HAXIS=S,HMIN=0.0,HMAX=6.0,VMIN=0.0,VMAX=10.0,VAXIS=BETX,BETY,FILE=plot;
